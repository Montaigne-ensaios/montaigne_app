<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textProfundidade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginTop="40dp"
                android:text="Profundidade:"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textAmostraN" />

            <EditText
                android:id="@+id/editTextProfundidade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="30dp"
                android:ems="10"
                android:inputType="number"
                android:text="xx"
                app:layout_constraintBottom_toBottomOf="@+id/textProfundidade"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textProfundidade"
                app:layout_constraintTop_toTopOf="@+id/textProfundidade" />

            <ImageButton
                android:id="@+id/imageButtonIcrementGolpe1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe1"
                app:layout_constraintStart_toEndOf="@+id/editTextGolpe1"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe1"
                app:srcCompat="@drawable/icon_increase" />

            <ImageButton
                android:id="@+id/imageButtonIcrementGolpe2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe2"
                app:layout_constraintStart_toEndOf="@+id/editTextGolpe2"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe2"
                app:srcCompat="@drawable/icon_increase" />

            <ImageButton
                android:id="@+id/imageButtonIcrementGolpe3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe3"
                app:layout_constraintStart_toEndOf="@+id/editTextGolpe3"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe3"
                app:srcCompat="@drawable/icon_increase" />

            <ImageButton
                android:id="@+id/imageButtonDecrementGolpe3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe3"
                app:layout_constraintEnd_toStartOf="@+id/editTextGolpe3"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe3"
                app:srcCompat="@drawable/icon_decrease" />

            <ImageButton
                android:id="@+id/imageButtonDecrementGolpe2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe2"
                app:layout_constraintEnd_toStartOf="@+id/editTextGolpe2"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe2"
                app:srcCompat="@drawable/icon_decrease" />

            <ImageButton
                android:id="@+id/imageButtonDecrementGolpe1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:background="#00FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe1"
                app:layout_constraintEnd_toStartOf="@+id/editTextGolpe1"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe1"
                app:srcCompat="@drawable/icon_decrease" />

            <TextView
                android:id="@+id/textAmostraN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:text="AMOSTRA N"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textGolpes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:text="Golpes"
                app:layout_constraintEnd_toEndOf="@+id/textProfundidade"
                app:layout_constraintStart_toStartOf="@+id/textProfundidade"
                app:layout_constraintTop_toBottomOf="@+id/textProfundidade" />

            <TextView
                android:id="@+id/textPenetracao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Penetração"
                app:layout_constraintBottom_toBottomOf="@+id/textGolpes"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.43"
                app:layout_constraintStart_toEndOf="@+id/textGolpes"
                app:layout_constraintTop_toTopOf="@+id/textGolpes"
                app:layout_constraintVertical_bias="1.0" />

            <EditText
                android:id="@+id/editTextGolpe1"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginTop="12dp"
                android:ems="10"
                android:inputType="number"
                android:text="Name"
                app:layout_constraintEnd_toEndOf="@+id/textGolpes"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintStart_toStartOf="@+id/textGolpes"
                app:layout_constraintTop_toBottomOf="@+id/textGolpes" />

            <EditText
                android:id="@+id/editTextPenetracao1"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginStart="24dp"
                android:ems="10"
                android:hint="15"
                android:inputType="number"
                app:layout_constraintBottom_toBottomOf="@+id/editTextGolpe1"
                app:layout_constraintStart_toEndOf="@+id/imageButtonIcrementGolpe1"
                app:layout_constraintTop_toTopOf="@+id/editTextGolpe1" />

            <EditText
                android:id="@+id/editTextPenetracao2"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:hint="15"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="@+id/editTextPenetracao1"
                app:layout_constraintStart_toStartOf="@+id/editTextPenetracao1"
                app:layout_constraintTop_toBottomOf="@+id/editTextPenetracao1" />

            <EditText
                android:id="@+id/editTextPenetracao3"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:hint="15"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="@+id/editTextPenetracao2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/editTextPenetracao2"
                app:layout_constraintTop_toBottomOf="@+id/editTextPenetracao2" />

            <EditText
                android:id="@+id/editTextGolpe2"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:inputType="number"
                android:text="0"
                app:layout_constraintEnd_toEndOf="@+id/editTextGolpe1"
                app:layout_constraintStart_toStartOf="@+id/editTextGolpe1"
                app:layout_constraintTop_toBottomOf="@+id/editTextGolpe1" />

            <EditText
                android:id="@+id/editTextGolpe3"
                android:layout_width="93dp"
                android:layout_height="41dp"
                android:layout_marginTop="16dp"
                android:ems="10"
                android:inputType="number"
                android:text="0"
                app:layout_constraintEnd_toEndOf="@+id/editTextGolpe2"
                app:layout_constraintStart_toStartOf="@+id/editTextGolpe2"
                app:layout_constraintTop_toBottomOf="@+id/editTextGolpe2" />

            <EditText
                android:id="@+id/editTextNivelAgua"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="250dp"
                android:ems="10"
                android:inputType="number"
                android:text="xx"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextGolpe3"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textNivelAgua"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                android:text="Nível de água"
                app:layout_constraintBottom_toBottomOf="@+id/editTextNivelAgua"
                app:layout_constraintEnd_toStartOf="@+id/editTextNivelAgua"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/imageButtonDecrementGolpe3"
                app:layout_constraintTop_toTopOf="@+id/editTextNivelAgua" />

            <TextView
                android:id="@+id/textSegmento"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:text="3 segmentos"
                app:layout_constraintBottom_toBottomOf="@+id/editTextPenetracao3"
                app:layout_constraintStart_toEndOf="@+id/editTextPenetracao2"
                app:layout_constraintTop_toTopOf="@+id/editTextPenetracao1" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>